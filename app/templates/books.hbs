<Breadcrumb />

<h1>Books Page</h1>

{{#each this.model.content as |stuff index|}}
{{stuff._record.name}}
    <LinkTo @route='book' @model={{stuff._record}}><li>Book title: {{stuff._record.name}}</li></LinkTo>
    <li>Id: {{stuff._record.id}}</li>
    <li>Author: {{stuff._record.authors}}</li>
    <li>Number of Pages: {{stuff._record.numberOfPages}}</li>
{{/each}}